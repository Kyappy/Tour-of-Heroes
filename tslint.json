{
	"extends": [
		"tslint-eslint-rules"
	],
	"rulesDirectory": [
		"node_modules/codelyzer"
	],
	"rules": {
		"adjacent-overload-signatures": true,
		"ban-types": [],
		"ban-comma-operator": true,
		"no-any": false,
		"no-import-side-effect": true,
		"no-internal-module": true,
		"no-magic-numbers": true,
		"no-namespace": true,
		"no-parameter-reassignment": false,
		"no-unnecessary-type-assertion": true,
		"no-var-requires": true,
		"no-reference": true,
		"only-arrow-functions": true,
		"no-inferrable-types": false,
		"typedef": [
			true,
			"call-signature",
			"arrow-call-signature",
			"parameter",
			"arrow-parameter",
			"property-declaration",
			"variable-declaration",
			"member-variable-declaration",
			"object-destructuring",
			"array-destructuring"
		],
		"await-promise": true,
		"promise-function-async": true,
		"prefer-for-of": true,
		"no-conditional-assignment": true,
		"arrow-return-shorthand": [true,
			"classes",
			"enums",
			"functions",
			"interfaces",
			"methods",
			"namespaces",
			"properties",
			"types",
			"variables"
		],
		"encoding": true,
		"file-header": false,
		"completed-docs": true,
		"callable-types": true,
		"class-name": true,
		"comment-format": [
			true,
			"check-space",
			"check-uppercase",
			{"ignore-words": ["region", "endregion", "console", "import"]}
		],
		"curly": [true, "ignore-same-line"],
		"eofline": false,
		"forin": true,
		"import-blacklist": [
			true,
			"rxjs",
			"rxjs/Rx"
		],
		"import-spacing": true,
		"interface-name": true,
		"indent": [true, "tabs"],
		"linebreak-style": [true, "CRLF"],
		"max-classes-per-file": [true, 1],
		"jsdoc-format": [true, "check-multiline-start"],
		"no-angle-bracket-type-assertion": true,
		"max-file-line-count": [true, 1000],
		"interface-over-type-literal": true,
		"match-default-export-name": true,
		"no-default-export": true,
		"newline-before-return": true,
		"new-parens": true,
		"no-redundant-jsdoc": true,
		"no-consecutive-blank-lines": [true, 2],
		"no-parameter-properties": false,
		"no-reference-import": true,
		"no-irregular-whitespace": true,
		"no-duplicate-imports": true,
		"no-mergeable-namespace": true,
		"no-require-imports": true,
		"no-boolean-literal-compare": true,
		"label-position": true,
		"max-line-length": [true, 140],
		"member-access": [true, "check-accessor", "check-constructor", "check-parameter-property"],
		"member-ordering": [
			true,
			{
				"order": [
					"public-static-field",
					"protected-static-field",
					"private-static-field",
					"public-instance-field",
					"protected-instance-field",
					"private-instance-field",
					"public-constructor",
					"protected-constructor",
					"private-constructor",
					"public-static-method",
					"protected-static-method",
					"private-static-method",
					"public-instance-method",
					"protected-instance-method",
					"private-instance-method"
				]
			}
		],
		"no-arg": true,
		"no-bitwise": true,
		"no-console": true,
		"no-construct": true,
		"no-debugger": true,
		"no-duplicate-super": true,
		"no-duplicate-switch-case": true,
		"no-duplicate-variable": true,
		"no-empty": true,
		"prefer-object-spread": true,
		"no-empty-interface": true,
		"no-eval": true,
		"no-floating-promises": true,
		"no-for-in-array": true,
		"no-implicit-dependencies": true,
		"no-inferred-empty-object-type": true,
		"no-invalid-template-strings": true,
		"no-invalid-this": true,
		"no-misused-new": true,
		"no-null-keyword": true,
		"no-object-literal-type-assertion": true,
		"no-return-await": true,
		"no-non-null-assertion": true,
		"no-shadowed-variable": true,
		"no-sparse-arrays": true,
		"no-string-literal": true,
		"no-submodule-imports": [
			true,
			"rxjs",
			"@angular",
			"core-js",
			"zone.js"
		],
		"no-this-assignment": true,
		"no-unbound-method": true,
		"no-unnecessary-class": true,
		"no-unsafe-any": true,
		"no-unsafe-finally": true,
		"no-string-throw": true,
		"no-unused-variable": true,
		"no-switch-case-fall-through": true,
		"no-trailing-whitespace": true,
		"no-unnecessary-initializer": true,
		"no-unnecessary-callback-wrapper": true,
		"no-unnecessary-qualifier": true,
		"no-unused-expression": true,
		"number-literal-format": true,
		"no-use-before-declare": true,
		"no-var-keyword": true,
		"no-void-expression": true,
		"prefer-conditional-expression": true,
		"object-literal-sort-keys": false,
		"restrict-plus-operands": true,
		"object-literal-key-quotes": [true, "as-needed"],
		"object-literal-shorthand": true,
		"strict-boolean-expressions": true,
		"strict-type-predicates": true,
		"switch-default": true,
		"ordered-imports": true,
		"prefer-function-over-method": false,
		"prefer-method-signature": true,
		"one-variable-per-declaration": true,
		"prefer-switch": [true, {"min-cases": 2}],
		"prefer-template": true,
		"one-line": [
			true,
			"check-open-brace",
			"check-whitespace"
		],
		"prefer-const": true,
		"trailing-comma": true,
		"align": true,
		"array-type": [true, "array"],
		"arrow-parens": true,
		"return-undefined": true,
		"quotemark": [true, "single"],
		"radix": true,
		"type-literal-delimiter": true,
		"space-in-parens": [true, "never"],
		"switch-final-break": true,
		"semicolon": [true, "always"],
		"space-before-function-paren": [true, "never"],
		"triple-equals": [true, "allow-null-check"],
		"typedef-whitespace": [
			true,
			{
				"call-signature": "nospace",
				"index-signature": "nospace",
				"parameter": "nospace",
				"property-declaration": "nospace",
				"variable-declaration": "nospace"
			}
		],
		"typeof-compare": true,
		"deprecation": true,
		"use-default-type-parameter": true,
		"unified-signatures": true,
		"variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
		"use-isnan": true,
		"cyclomatic-complexity": true,
		"whitespace": [
			true,
			"check-branch",
			"check-decl",
			"check-operator",
			"check-separator",
			"check-type",
			"check-typecast",
			"check-preblock"
		],
		"directive-selector": [true, "attribute", "app", "camelCase"],
		"component-selector": [true, "element", "app", "kebab-case"],
		"use-input-property-decorator": true,
		"use-output-property-decorator": true,
		"use-host-property-decorator": true,
		"no-input-rename": false,
		"no-output-rename": true,
		"use-life-cycle-interface": true,
		"use-pipe-decorator": true,
		"use-pipe-transform-interface": true,
		"component-class-suffix": [true, "Component", "View"],
		"directive-class-suffix": true,
		"invoke-injectable": true,
		"banana-in-box": true,
		"no-access-missing-member": true,
		"pipe-impurer": true,
		"templates-no-negated-async": true,
		"templates-use-public": true,
		"template-to-ng-template": true,
		"no-attribute-parameter-decorator": true,
		"no-forward-ref": true,
		"no-used-css": true,
		"use-view-encapsulation": true,
		"angular-whitespace": [true, "check-interpolation"],
		"import-destructing-spacing": true,
		"pipe-naming": ["camelCase", ""],
		"no-constant-condition": true,
		"no-control-regex": true,
		"no-duplicate-case": true,
		"no-empty-character-class": true,
		"no-ex-assign": true,
		"no-extra-boolean-cast": true,
		"no-extra-semi": true,
		"no-inner-declarations": true,
		"no-invalid-regexp": true,
		"ter-no-irregular-whitespace": true,
		"no-regex-spaces": true,
		"no-unexpected-multiline": true,
		"valid-jsdoc": [true, {
			"prefer": {
				"return": "returns"
			},
			"requireReturn": true,
			"requireParamDescription": true,
			"requireReturnDescription": true,
			"matchDescription": "^[A-Z][A-Za-z0-9\\s,`]*[.]$"
		}],
		"valid-typeof": true,
		"no-multi-spaces": true,
		"handle-callback-err": true,
		"array-bracket-spacing": [true, "never"],
		"block-spacing": [
			true,
			"always"
		],
		"brace-style": [
			true,
			"stroustrup",
			{
				"allowSingleLine": true
			}
		],
		"ter-computed-property-spacing": true,
		"ter-func-call-spacing": true,
		"ter-indent": [true, "tab"],
		"ter-max-len": [true, 140],
		"object-curly-spacing": [
			true,
			"never"
		],
		"ter-no-tabs": true
	}
}